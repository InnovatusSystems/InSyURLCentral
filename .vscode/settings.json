{
  "dotnet.defaultSolution": "InSyURLCentral.sln",
  "dotnet.watchFileExtensions": [".cs", ".cshtml", ".json"],
  "files.exclude": {
    "**/bin": true,
    "**/obj": true
  },
  "files.trimTrailingWhitespace": true,
  "editor.formatOnSave": false,
  "editor.tabSize": 4,
  "editor.insertSpaces": true,
  "csharp.format.enable": true,
  "csharp.format.indentationSize": 4,
  "csharp.format.tabSize": 4,
  "csharp.format.useTabs": false,
  "dotnet.codeLens.enableReferencesCodeLens": true,
  "dotnet.codeLens.enableTestsCodeLens": true
}
